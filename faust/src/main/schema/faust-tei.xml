<?xml version="1.0"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:lang="en"
	xmlns:rng="http://relaxng.org/ns/structure/1.0"
	xmlns:ge="http://www.tei-c.org/ns/geneticEditions" xmlns:svg="http://www.w3.org/2000/svg" xmlns:f="http://www.faustedition.net/ns">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Digitale Faust-Edition – Faust TEI Schema</title>
				<author>Moritz Wissenbach</author>
				<author>Gregor Middell</author>
			</titleStmt>
			<publicationStmt>
				<p>for internal use</p>
			</publicationStmt>
			<notesStmt>
				<note type="ns">http://www.tei-c.org/ns/1.0</note>
			</notesStmt>
			<sourceDesc>
				<p/>
			</sourceDesc>
		</fileDesc>
	</teiHeader>
	<text>
		<front>
			<divGen type="toc"/>
		</front>
		<body>
			<div>
				<head>Allgemeine Kodierungsprinzipien</head>
				
				<div>
					<head>Anchors und Segmentierung für überlappendes Markup</head>
					
					<list>
						<item><ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/SA.html#SASE"/></item>
					</list>
				</div>
				
				<div>
					<head>Kollation/ Allignierung</head>
					
					<list>
						<item>Guides: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/SA.html#SACS"/></item>
					</list>
				</div>
			</div>
			<div>
				<head>Dokumentarisches Markup</head>
				
				<list>
					<item><ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHCH"/></item>
				</list>
				
				<div>
					<head>Textuelle Varianz in der Transkription</head>
					
					<list>
						<item><ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/TC.html#TCTR"/></item>
					</list>
				</div>
				
				<div>
					<head>Unsicherheit</head>

					<p>nicht mit <code>certainty</code>, da strukturierte Informationen nicht hinterlegt</p>
					
					<list>
						<item>mittels <code>note</code> und <code>@target</code>: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/CE.html#CECENO"/></item>
					</list>
				</div>
			</div>
			<div>
				<head>Gattungsspezifisches Markup: Drama</head>
				
				<div>
					<head>Wer spricht</head>
					
					<list>
						<item><ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/DR.html#DRSP"/></item>
						<item><ptr target="http://wiki.faustedition.net/index.php/Namenssiglen"/></item>
					</list>
				</div>
				
				<div>
					<head>Überlappende Hierarchien</head>
					
					<list>
						<item>Versgruppen verbinden: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/DR.html#DREMB"/></item>
						<item>Überlappendes Markup mit <code>anchor</code> und <code>@subtype</code> für konformes TEI: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/NH.html#NHBM"/></item>
					</list>
				</div>
			</div>
			<div>
				<head>Linguistisches Markup</head>
				
				<list>
					<item>Satzstruktur: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/AI.html#AILC"/></item>
				</list>
			</div>
			<div>
				<head>Genetisches Markup</head>
				
				<div>
					<head>Pfade</head>

					<list>
						<item>Graphen: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/GD.html"/></item>
					</list>
				</div>
			</div>
			<div>
				<head>Metadaten/ Manuskriptbeschreibung</head>
				
				<list>
					<item>TEI-MS: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/MS.html"/></item>
					<item>Faust:<ptr target="http://wiki.faustedition.net/index.php/Metadatenerfassung"/></item>
					<item>Facsimile-Verlinkung: <ptr target="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHFAX"/></item>
				</list>
			</div>
			<div>
				<p>TEI Extension for Faust&#xD; &#xD; preliminary version</p>
				<schemaSpec ident="faust-tei" docLang="en" prefix="tei_" start="TEI" xml:lang="en"
					targetLang="en">
					<moduleRef key="core"/>
					<moduleRef key="tei"/>
					<moduleRef key="header"/>
					<moduleRef key="textstructure"/>
					<moduleRef key="certainty"/>
					<moduleRef key="analysis"/>
					<moduleRef key="drama"/>
					<moduleRef key="iso-fs"/>
					<moduleRef key="linking"/>
					<moduleRef key="msdescription"/>
					<moduleRef key="nets"/>
					<moduleRef key="textcrit"/>
					<moduleRef key="transcr"/>
					<moduleRef key="verse"/>
					<moduleRef key="gaiji"/>
					<moduleRef key="namesdates"/>
					<moduleRef key="figures"/>
					<moduleRef url="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/svg11.rng">
						<content>
							<rng:define name="tei_model.graphicLike" combine="choice">
								<rng:ref name="svg"/>
							</rng:define>
						</content>
					</moduleRef>

					<!-- ================== TEI-SIG Genetic Editions: START ================== -->

					<!-- add new staged class for anything which can take @stage -->
					<classSpec ident="att.staged" type="atts" mode="add" module="tei">
						<desc>groups elements which can be assigned to a specific text stage by
							means of the attributes it provides.</desc>

						<attList>
							<attDef ident="stage" mode="add">
								<desc> points to a <gi>stageNote</gi> which contains a description
									of a text-stage to which the editors think the alteration marked
									by the element bearing this attribute (and its children)
									belongs. </desc>
								<datatype minOccurs="1" maxOccurs="1">
									<rng:choice>
										<rng:data type="anyURI"/>
										<!--rng:ref name="data.pointer"/-->
										<rng:value>0</rng:value>
									</rng:choice>
								</datatype>
								<remarks>
									<p>The value 0 indicates that the element concerned is
										considered to belong to the same stage as its sibling
										elements.</p>
								</remarks>
							</attDef>
						</attList>
					</classSpec>


					<!-- add existing transcriptional elements to the staged class -->
					<classSpec ident="att.transcriptional" type="atts" mode="change" module="tei">
						<classes>
							<memberOf key="att.staged"/>
							<memberOf key="att.editLike"/>
						</classes>
					</classSpec>


					<classSpec ident="model.zonePart" type="model" mode="add">
						<desc>elements which can form part of a zone</desc>
					</classSpec>

					<classSpec ident="model.linePart" type="model" mode="add">
						<desc>elements which can form part of a line</desc>
					</classSpec>


					<!-- topographical document structure -->

					<elementSpec ident="document" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>contains a document-centric transcription of a primary source,
							providing topographical information as well as transcription</desc>
						<classes>
							<memberOf key="model.resourceLike"/>
							<memberOf key="att.global"/>
						</classes>
						<content>
							<rng:oneOrMore>
								<rng:ref name="surface"/>
							</rng:oneOrMore>
						</content>
					</elementSpec>

					<elementSpec ident="surface" module="physdesc" mode="change">
						<classes mode="change">
							<memberOf key="att.typed" mode="add"/>
						</classes>
						<content>
							<rng:zeroOrMore>
								<rng:choice>
									<rng:ref name="model.global"/>
									<rng:ref name="model.glossLike"/>
									<rng:ref name="model.graphicLike"/>
									<rng:ref name="zone"/>
									<rng:ref name="line"/>
									<rng:ref name="patch"/>
								</rng:choice>
							</rng:zeroOrMore>
						</content>
					</elementSpec>

					<elementSpec ident="zone" module="physdesc" mode="change">
						<classes mode="change">
							<!--memberOf key="att.spanning" mode="add"/-->
							<memberOf key="att.coordinated" mode="add"/>
							<memberOf key="model.zonePart" mode="add"/>
							<!--memberOf key="att.transcriptional" mode="add"/-->
							<memberOf key="att.staged" mode="add"/>
						</classes>
						<content>
							<rng:zeroOrMore>
								<rng:choice>
									<rng:text/>
									<rng:ref name="model.zonePart"/>
									<rng:ref name="model.global"/>
									<rng:ref name="model.pPart.transcriptional"/>
									<rng:ref name="model.gLike"/>
								</rng:choice>
							</rng:zeroOrMore>
						</content>
						<attList>
							<!--
							<attDef ident="ref" mode="add"> <desc> A reference to an arbitrary object that describes the placement and extent of the zone (such as an SVG element in an external file). The use of this
							attribute is exclusive of giving x/y-coordinates directly. </desc> <datatype minOccurs="1" maxOccurs="1"> <rng:ref name="data.pointer"/> </datatype> </attDef>
						-->
							<attDef ident="rotate" mode="add">
								<desc> indicates the amount by which this zone has been rotated
									clockwise, with respect to the normal orientation of the parent
										<gi>surface</gi> element as implied by the dimensions given
									in the <gi>msDesc</gi> section or by the coordinates of the
										<gi>surface</gi> itself. The orientation is expressed in arc
									degrees. </desc>
								<datatype minOccurs="1" maxOccurs="1">
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.count"/>
								</datatype>
								<defaultVal>0</defaultVal>
							</attDef>
							<!--
							<attDef ident="patch" mode="add"> <desc>Describe the type of patch</desc> <datatype> <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.word"/> </datatype> </attDef> <attDef
							ident="binder" mode="add"> <desc>Describe the method by which a patch is connected to the main surface</desc> <datatype> <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.word"/>
							</datatype> </attDef> <attDef ident="heigth"> <desc>The height of the patch</desc> <datatype> <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.numerical"/> </datatype> </attDef>
							<attDef ident="width"> <desc>The width of the patch</desc> <datatype> <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.numerical"/> </datatype> </attDef>
						-->
						</attList>
					</elementSpec>

					<!-- add milestone to att.spanning (this should be done for all model.milestoneLike elements actually -->
					<elementSpec ident="milestone" module="core" mode="change">
						<classes>
							<memberOf key="att.spanning"/>
							<memberOf key="model.milestoneLike"/>
							<memberOf key="att.typed"/>
							<memberOf key="att.sourced"/>
						</classes>
					</elementSpec>

					<elementSpec ident="patch" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>contains a part of a written surface which was originally physically
							distinct but became attached to it at the time that one or more written
							zones were created on it.</desc>
						<classes>
							<memberOf key="att.coordinated"/>
							<memberOf key="att.global"/>
							<memberOf key="att.typed"/>
							<!-- should this be staged? -->
						</classes>
						<content>
							<rng:zeroOrMore>
								<rng:choice>
									<rng:text/>
									<rng:ref name="zone"/>
									<rng:ref name="model.global"/>
								</rng:choice>
							</rng:zeroOrMore>
						</content>

						<attList>
							<attDef ident="binder" mode="add">
								<desc>Describe the method by which a patch is or was connected to
									the main surface</desc>
								<datatype>
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.enumerated"/>
								</datatype>

								<valList type="open">
									<valItem ident="glue">
										<desc>patch is glued in place</desc>
									</valItem>
									<valItem ident="pin">
										<desc>patch is pinned or stapled in place</desc>
									</valItem>
									<valItem ident="sewn">
										<desc>patch is sewn in place</desc>
									</valItem>
								</valList>
							</attDef>

							<attDef ident="flipping" mode="add">
								<desc>indicates whether the patch is attached and folded in such a
									way as to provide two writing surfaces</desc>
								<datatype>
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.truthValue"/>
								</datatype>

							</attDef>
							<attDef ident="height">
								<desc>height of the patch in mm</desc>
								<datatype>
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.numeric"/>
								</datatype>
							</attDef>
							<attDef ident="width">
								<desc>width of the patch in mm</desc>
								<datatype>
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.numeric"/>
								</datatype>
							</attDef>

						</attList>
					</elementSpec>

					<macroSpec ident="macro.lineContent" type="pe">
						<content>
							<rng:zeroOrMore>
								<rng:choice>
									<rng:text/>
									<rng:ref name="model.global"/>
									<rng:ref name="model.linePart"/>
									<rng:ref name="model.pPart.transcriptional"/>
									<rng:ref name="model.pPart.editorial"/>
									<!-- not sure if thats wise -->
									<rng:ref name="model.segLike"/>
									<!-- not sure if thats wise -->
									<rng:ref name="model.gLike"/>
									<rng:ref name="model.hiLike"/>
								</rng:choice>
							</rng:zeroOrMore>
						</content>
					</macroSpec>
					
					<elementSpec ident="line" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>contains the transcription of a topographic line in the source
							document</desc>
						<classes>
							<memberOf key="model.zonePart"/>
							<memberOf key="att.staged"/>
							<memberOf key="att.global"/>
							<memberOf key="att.typed"/>
						</classes>
						<content>
							<rng:ref name="macro.lineContent"/>
						</content>
					</elementSpec>

					<elementSpec ident="creation" mode="change">
						<content>
							<rng:zeroOrMore>
								<rng:ref name="macro.phraseSeq.limited"/>
							</rng:zeroOrMore>
							<rng:zeroOrMore>
								<rng:ref name="stageHist"/>
							</rng:zeroOrMore>
						</content>
					</elementSpec>

					<elementSpec ident="stageHist" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>contains one or more descriptions of the stages which have been
							identified in the genesis of a text.</desc>
						<classes>
							<memberOf key="att.global"/>

						</classes>
						<content>
							<rng:choice xmlns="http://relaxng.org/ns/structure/1.0">
								<rng:oneOrMore>
									<rng:ref name="model.pLike"/>
								</rng:oneOrMore>
								<rng:group>
									<rng:optional>
										<rng:ref name="summary"/>
									</rng:optional>
									<rng:oneOrMore>
										<rng:ref name="change"/>
									</rng:oneOrMore>
								</rng:group>
							</rng:choice>
						</content>
					</elementSpec>

					<elementSpec ident="change" module="header" mode="change">
						<desc>documents a particular stage in the genesis of a text.</desc>
						<content>
							<rng:ref name="macro.specialPara"/>
						</content>
					</elementSpec>


					<elementSpec ident="mod" ns="http://www.tei-c.org/ns/geneticEditions" mode="add">
						<desc>defines the scope of an area in the document containing several
							alterations which are considered as belonging to the same revision
							campaign. </desc>
						<classes>
							<memberOf key="model.pPart.transcriptional"/>
							<memberOf key="att.spanning"/>
							<memberOf key="att.staged"/>
							<memberOf key="att.global"/>

						</classes>
						<content>
							<rng:empty/>
						</content>
					</elementSpec>


					<!-- authorial changes -->
					<elementSpec ident="metaMark" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>(meta mark) A textual or graphical element in a manuscript that is
							functional but not part of the text. Could transform the text, like a
							strikethrough, or provide meta-information, like a date.</desc>
						<classes>
							<memberOf key="model.pPart.transcriptional"/>
							<memberOf key="att.spanning"/>
							<memberOf key="att.placement"/>
							<memberOf key="att.staged"/>
							<memberOf key="att.global"/>

						</classes>
						<content>
							<ref xmlns="http://relaxng.org/ns/structure/1.0"
								name="macro.specialPara"/>
						</content>
						<attList>
							<attDef ident="function" mode="add">
								<desc>describes the function (e.g. add, delete, alternate) of the
									mark.</desc>
								<datatype minOccurs="1">
									<rng:ref name="data.word"/>
								</datatype>
							</attDef>
							<attDef ident="targets">
								<desc>indicates the element(s) to which the function of the
									meta-mark refers. Pointers are separated by a white space</desc>
								<datatype minOccurs="1" maxOccurs="unbounded">
									<ref xmlns="http://relaxng.org/ns/structure/1.0"
										name="data.pointer"/>
								</datatype>
							</attDef>
						</attList>
					</elementSpec>

					<elementSpec ident="rewrite" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>contains a sequence of text which has been rewritten by the author,
							for example by over-inking, to clarify or fix it.</desc>
						<classes>
							<memberOf key="model.pPart.transcriptional"/>
							<memberOf key="model.global"/>
							<memberOf key="att.spanning"/>
							<memberOf key="att.global"/>
							<memberOf key="att.transcriptional"/>

						</classes>
						<content>
							<rng:ref name="macro.paraContent"/>
						</content>
						<attList>
							<attDef ident="cause">
								<desc>documents the presumed cause of the rewriting.</desc>
								<datatype>
									<rng:ref name="data.enumerated"/>
								</datatype>
								<valList type="closed">
									<valItem ident="fix">
										<desc>rewriting for the purpose of fixation</desc>
									</valItem>
									<valItem ident="unclear">
										<desc>rewriting to clarify a previously illegible or badly
											written sequence</desc>
									</valItem>
								</valList>
							</attDef>
						</attList>
						<remarks>
							<p> Multiple rewritings are indicated by nesting one <gi>rewrite</gi>
								within another. In principle, a rewriting differs from a
								substitution in that second and subsequent rewrites do not
								materially alter the content of an element. Where there are minor
								changes made during the rewriting however these may be marked up
								using <gi>del</gi> , <gi>add</gi> , etc. with an appropriate value
								for the <att>stage</att> attribute. </p>
						</remarks>
					</elementSpec>



					<elementSpec ident="used" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>In many cases, authors mark portions of text as having been used,
							usually meaning the text has been transcribed to a fair copy. The mark
							is often a strikethrough, but can be any author-specific mark.</desc>
						<classes>
							<memberOf key="model.pPart.transcriptional"/>
							<memberOf key="model.global"/>
							<memberOf key="att.spanning"/>
							<memberOf key="att.staged"/>
							<memberOf key="att.global"/>

						</classes>
						<content>
							<rng:empty/>
						</content>
					</elementSpec>
					<elementSpec ident="undo" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>Marks up an action represented by an element to be undone.</desc>
						<classes>
							<memberOf key="model.pPart.transcriptional"/>
							<memberOf key="att.spanning"/>
							<memberOf key="att.staged"/>
							<memberOf key="att.global"/>

						</classes>
						<content>
							<rng:empty/>
						</content>

						<attList>
							<attDef ident="target" mode="add">
								<desc>The element representing the action to be undone.</desc>
								<datatype minOccurs="1" maxOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
						</attList>
					</elementSpec>

					<elementSpec ident="unclear" module="core" mode="change">
						<classes mode="replace">
							<memberOf key="model.choicePart" mode="add"/>
							<memberOf key="model.pPart.transcriptional" mode="add"/>
							<memberOf key="att.editLike" mode="add"/>
							<memberOf key="att.typed" mode="add"/>
						</classes>
					</elementSpec>

					<elementSpec ident="transposeGrp" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>supplies a list of transpositions indicated at some point in the text,
							typically by means of metamarks.</desc>
						<classes>
							<memberOf key="model.profileDescPart"/>
							<memberOf key="model.global.meta"/>
							<memberOf key="att.global"/>
						</classes>
						<content>
							<rng:oneOrMore>
								<rng:ref name="transpose"/>
							</rng:oneOrMore>
						</content>
					</elementSpec>

					<elementSpec ident="transpose" ns="http://www.tei-c.org/ns/geneticEditions"
						mode="add">
						<desc>describes a single textual transposition as an ordered list of at
							least two pointers specifying the order in which the elements indicated
							should be re-combined. </desc>
						<classes>
							<memberOf key="att.global"/>
						</classes>
						<content>
							<rng:group>
								<rng:ref name="ptr"/>
								<rng:oneOrMore>
									<rng:ref name="ptr"/>
								</rng:oneOrMore>
							</rng:group>
						</content>
					</elementSpec>

					<!-- genetic groups -->
					<elementSpec ident="geneticGrp" mode="add"
						ns="http://www.tei-c.org/ns/geneticEditions">
						<desc>Group texts and document which are somehow related in a genetic
							process</desc>
						<classes>
							<memberOf key="model.profileDescPart"/>
							<memberOf key="att.global"/>
						</classes>
						<content>
							<rng:oneOrMore>
								<rng:ref name="geneticNote"/>
							</rng:oneOrMore>
						</content>
					</elementSpec>

					<elementSpec ident="geneticNote" mode="add"
						ns="http://www.tei-c.org/ns/geneticEditions">
						<desc>describes a particular set of documents or document fragments which
							are considered to be mutually associated in some way.</desc>
						<classes>
							<memberOf key="att.typed"/>
							<memberOf key="att.editLike"/>
							<memberOf key="att.global"/>
						</classes>
						<content>
							<rng:oneOrMore>
								<rng:ref name="linkGrp"/>
							</rng:oneOrMore>
							<rng:oneOrMore>
								<rng:ref name="model.pLike"/>
							</rng:oneOrMore>
						</content>
					</elementSpec>

					<!-- timeline -->

					<!--
					<elementSpec ident="timeline" mode="change"> <classes mode="change"> <memberOf key="model.choicePart" mode="add"/> </classes> <content> <rng:oneOrMore> <rng:ref name="when"/> <rng:zeroOrMore>
					<rng:ref name="choice"/> </rng:zeroOrMore> </rng:oneOrMore> </content> <attList> <attDef ident="weights"> <datatype> <rng:ref name="data.probability" /> </datatype> </attDef> </attList>
					</elementSpec> <elementSpec ident="when" mode="change"> <classes> <memberOf key="att.editLike"/> </classes> <attList> <attDef ident="target" mode="add"> <datatype> <rng:ref name="data.pointer" />
					</datatype> </attDef> <attDef ident="unit" mode="delete"/> <attDef ident="interval" mode="replace"> <datatype> <rng:choice> <rng:ref name="data.numeric"/> <rng:value>unknown</rng:value>
					</rng:choice> </datatype> </attDef> </attList> </elementSpec>
				-->

					<!-- spanning text structure -->

					<elementSpec ident="p" mode="change">
						<classes mode="change">
							<memberOf mode="add" key="att.spanning"/>
							<memberOf mode="add" key="model.global"/>
						</classes>
					</elementSpec>

					<!-- ================== TEI-SIG Genetic Editions: END ================== -->

					<classSpec type="atts" ident="att.global" mode="change">
						<classes mode="change">
							<memberOf key="att.xmlspace"/>
						</classes>
					</classSpec>
					
					<elementSpec ident="fw" module="transcr" mode="change">
						<classes mode="change">
							<memberOf key="att.transcriptional"/>
						</classes>
					</elementSpec>
					
					<elementSpec ident="space" module="transcr" mode="change">
						<classes mode="change">
							<memberOf key="att.typed"/>
						</classes>
					</elementSpec>

					<classSpec ident="model.pPart.msdesc" type="model" mode="change">
						<classes mode="change">
							<memberOf key="model.linePart"/>
						</classes>
					</classSpec>
					
					<elementSpec ident="line" ns="http://www.tei-c.org/ns/geneticEditions" mode="change">
						<attList>
							<attDef ident="type" mode="change">
								<valList type="closed"  mode="add">
									<valItem ident="inter"><gloss>interlineare Zeile</gloss></valItem>
									<valItem ident="uncounted"><gloss>nicht gezählte Zeile</gloss></valItem>
								</valList>
							</attDef>
						</attList>	
					</elementSpec>
					
					<classSpec ident="att.alignable" type="atts" mode="add">
						<desc>Definiert Attribute zur topographischen Alignierung von (graphischen) Elementen.</desc>
						<classes>
							<memberOf>att.global</memberOf>
						</classes>
						<attList>
							<attDef ident="align-top-with" ns="http://www.faustedition.net/ns" usage="opt">
								<desc>Aligniert die obere Kante des Elements mit der oberen Kante des referenzierten.</desc>
								<datatype minOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
							<attDef ident="align-bottom-with" ns="http://www.faustedition.net/ns" usage="opt">
								<desc>Aligniert die untere Kante des Elements mit der unteren Kante des referenzierten.</desc>
								<datatype minOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
							<attDef ident="align-left-with" ns="http://www.faustedition.net/ns" usage="opt">
								<desc>Aligniert die linke Kante des Elements mit der linken Kante des referenzierten.</desc>
								<datatype minOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
							<attDef ident="align-right-with" ns="http://www.faustedition.net/ns" usage="opt">
								<desc>Aligniert die rechte Kante des Elements mit der rechten Kante des referenzierten.</desc>
								<datatype minOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
						</attList>
					</classSpec>
					
					<elementSpec ident="stroke" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Kodiert ein graphisches Element auf Dokumentebene</desc>
						<classes>
							<memberOf key="model.global"/>
							<memberOf key="att.alignable"/>
						</classes>
						<content>
							<rng:empty/>
						</content>
						<attList>
							<attDef ident="type" mode="add">
								<desc>Typ des graphischen Elements</desc>
								<valList type="closed">
									<valItem ident="horiz_line"><gloss>horizontale gerade Linie</gloss></valItem>
									<valItem ident="vert_line"><gloss>vertikale gerade Linie</gloss></valItem>
									<valItem ident="horiz_curly_line"><gloss>horizontale geschwungene Linie</gloss></valItem>
									<valItem ident="vert_curly_line"><gloss>vertikale geschwungene Linie</gloss></valItem>
									
									<valItem ident="horiz_brace"><gloss>horizontale gerade Klammer</gloss></valItem>
									<valItem ident="vert_brace"><gloss>vertikale gerade Klammer</gloss></valItem>									
									<valItem ident="horiz_curly_brace"><gloss>horizontale geschwungene Klammer</gloss></valItem>
									<valItem ident="vert_curly_brace"><gloss>vertikale geschwungene Klammer</gloss></valItem>									
								</valList>
							</attDef>
						</attList>
					</elementSpec>
					
					<elementSpec ident="ins" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Kodiert Einfügungszeichen.</desc>
						<classes>
							<memberOf key="model.linePart"/>
							<memberOf key="att.alignable"/>
						</classes>
						<attList>
							<attDef ident="at" usage="opt">
								<desc>Verweist auf die Stelle, an der etwas eingefügt wird.</desc>
								<datatype minOccurs="1" maxOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
						</attList>
					</elementSpec>
					
					<elementSpec ident="st" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Beschreibt eine Durchstreichung („strikethrough”) auf Dokumentebene.</desc>
						<classes>
							<memberOf key="model.zonePart"/>
							<memberOf key="model.linePart"/>
							<memberOf key="att.spanning"/>
						</classes>
						<content>
							<rng:ref name="macro.lineContent"/>
						</content>
						<attList>
							<attDef ident="rend" usage="opt">
								<valList type="closed" mode="add">
									<valItem ident="double"><gloss>doppelte Streichung</gloss></valItem>
									<valItem ident="double"><gloss>mehrfache Streichung</gloss></valItem>
									<valItem ident="vertical"><gloss>vertikal durchgestrichen</gloss></valItem>
									<valItem ident="erase"><gloss>Rasur</gloss></valItem>
								</valList>
							</attDef>
						</attList>
					</elementSpec>
					
					<classSpec ident="model.linePart.overwritten" type="model"/>
					
					<elementSpec ident="on" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Das unter der Überschreibung stehende.</desc>
						<classes>
							<memberOf key="model.linePart.overwritten"/>
						</classes>
						<content>
							<rng:ref name="macro.lineContent"/>
						</content>
					</elementSpec>

					<elementSpec ident="sup" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Das auf dem Überschriebenden stehende.</desc>
						<classes>
							<memberOf key="model.linePart.overwritten"/>
						</classes>
						<content>
							<rng:ref name="macro.lineContent"/>
						</content>
					</elementSpec>
					
					<elementSpec ident="overw" ns="http://www.faustedition.net/ns" mode="add">
						<desc>Umgebung zur Kodierung von Überschreibungen.</desc>
						<classes>
							<memberOf key="model.linePart"/>
							<memberOf key="model.linePart.overwritten"/>
							<memberOf key="att.staged"/>							
						</classes>
						<content>
							<rng:group>
								<rng:group>
									<rng:ref name="model.linePart.overwritten"/>
								</rng:group>
								<rng:oneOrMore>
									<rng:ref name="model.linePart.overwritten"/>
								</rng:oneOrMore>
							</rng:group>
						</content>
					</elementSpec>
					
					<classSpec ident="att.changeRelated" type="atts" mode="add">
						<attList>
							<attDef ident="revType" ns="http://www.faustedition.net/ns" usage="opt" mode="add">
								<valList type="closed">
									<valItem ident="instant"><gloss>Sofortrevision</gloss></valItem>
									<valItem ident="soon"><gloss>Baldrevision</gloss></valItem>
									<valItem ident="late"><gloss>Spätrevision</gloss></valItem>
									<valItem ident="uncertain"><gloss>nicht zuzuordnen</gloss></valItem>
								</valList>
							</attDef>
							<attDef ident="changeRef" ns="http://www.faustedition.net/ns" usage="opt" mode="add">
								<datatype minOccurs="1" maxOccurs="1">
									<rng:ref name="data.pointer"/>
								</datatype>
							</attDef>
						</attList>
					</classSpec>
					<elementSpec ident="add" mode="change">
						<classes mode="change">
							<memberOf key="att.changeRelated"/>
