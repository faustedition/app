<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/</from>
		<to type="redirect">project/about</to>
	</rule>
	<rule>
		<from>/login</from>
		<to type="redirect">text/</to>
	</rule>
	<rule>
		<from>/dav</from>
		<to type="redirect">dav/</to>
	</rule>
	<rule>
		<from>/css/**</from>
		<to>/css/$1</to>
	</rule>
	<rule>
		<from>/img/**</from>
		<to>/img/$1</to>
	</rule>
	<rule>
		<from>/js/**</from>
		<to>/js/$1</to>
	</rule>
	<rule>
		<from>/static/**</from>
		<to>/static/$1</to>
	</rule>
	<rule>
		<from>/swf/**</from>
		<to>/swf/$1</to>
	</rule>
	<rule>
		<from>/dav/**</from>
		<to>/dav/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/spring/$1</to>
	</rule>
	<outbound-rule>
		<from>/spring/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>